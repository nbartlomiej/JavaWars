<?xml version="1.0" encoding="UTF-8"?>
<module>
	
        <inherits name="com.google.gwt.user.User"/>
        
        <!-- required for xml support -->
        <inherits name="com.google.gwt.xml.XML"/> 
        
        <!-- hibernate4gwt, for operating on the same objects on
        server and client side -->
        <inherits name='net.sf.hibernate4gwt.Hibernate4Gwt15'/>
        
        <entry-point class="javawars.client.MainEntryPoint"/>

        <!-- the css view descriptions -->
        <stylesheet src="../styles/menu.css"/>
        <stylesheet src="../styles/matchviewer.css"/>

       <!-- Additional source path -->
       <source path='domain'/>
       <source path='client'/> 

	
</module>
